<Window x:Class="WPFTextGUI.Webcheck.WebControlWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFTextGUI.Webcheck"
        mc:Ignorable="d"
        Title="WebControlWindow" Height="400" Width="500">
    <Grid>
        <TextBox x:Name="txtUrl" FontSize="24" HorizontalAlignment="Left" Margin="51,51,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="407"/>
        <TextBox x:Name="txtTerm" FontSize="24" HorizontalAlignment="Left" Margin="51,137,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="407"/>
        <Button x:Name="btnStart" FontSize="24" Width="100" Content="Start" HorizontalAlignment="Left" Margin="54,208,0,0" VerticalAlignment="Top" Click="btnStart_Click"/>
        <Button x:Name="btnStop" FontSize="24" Width="100" Content="Stop" HorizontalAlignment="Center" Margin="0,208,0,0" VerticalAlignment="Top" Click="btnStop_Click"/>
        <Label Content="Url" HorizontalAlignment="Left" FontSize="20" Margin="54,14,0,0" VerticalAlignment="Top"/>
        <Label Content="Search&#xD;&#xA;" HorizontalAlignment="Left" FontSize="20" Margin="51,104,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.17,0.311" Height="33"/>

    </Grid>
</Window>
